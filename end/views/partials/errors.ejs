<% if (errors.length> 0) { %>
    <div class="bg-red-400 border-4 border-black p-4 mb-4">
        <h3 class="font-bold text-black">VALIDATION INPUT:</h3>
        <ul class="list-disc list-inside text-black">
            <% errors.forEach(error=> { %>
                <li>
                    <%= error %>
                </li>
                <% }) %>
        </ul>
    </div>
    <% } %>